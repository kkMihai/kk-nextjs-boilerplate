{
  "compilerOptions": {
    "target": "es2021",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
        "@/components/*": ["./src/components/*"],
        "@/pages/*": ["./src/pages/*"],
        "@/styles/*": ["./src/styles/*"],
        "@/lib/*": ["./src/lib/*"],
        "@/hooks/*": ["./src/hooks/*"],
        "@/schemas/*": ["./src/schemas/*"],
        "@/Auth/options.js": ["./src/pages/api/auth/[...nextauth].js"],
        "@/Auth.js": ["./src/lib/auth/auth.js"],
        "@/Auth/constants.js": ["./src/lib/auth/constants.js"],
        "@/env": ["./src/env.mjs"],
        "@/keys/*": ["./.keys/*"]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": ["node_modules", ".next"]
}
