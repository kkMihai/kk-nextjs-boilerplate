{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "importExtensions": [".js", ".jsx", ".mjs"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
